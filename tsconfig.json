{
"extends": "./.svelte-kit/tsconfig.json",
"compilerOptions": {
    "moduleResolution": "bundler",
    "target": "ES2020",
    "module": "ES2020",
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
        "$lib": ["./src/lib"],
        "$lib/*": ["./src/lib/*"]
    },
    "isolatedModules": true
}
}

